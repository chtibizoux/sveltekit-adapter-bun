{
  "name": "@chtibizoux/sveltekit-adapter-bun",
  "description": "Another sveltekit adapter for bun.",
  "keywords": [
    "sveltekit",
    "adapter",
    "bun"
  ],
  "version": "0.0.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chtibizoux/svelte-adapter-bun.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun ./build.ts"
  },
  "bin": {
    "sveltekit-bun": "dist/bin.js"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.6.1",
    "@types/bun": "latest",
    "cac": "^6.7.14",
    "devalue": "^5.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rollup": "^4.22.5"
  }
}
